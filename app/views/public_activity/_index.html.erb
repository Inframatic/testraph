		<h3>Activity Feed</h3>
		<% if @activities.empty? %>
		
		<h4>Your not following anyone!</h4>
		
		<% else %>
		
		<% @activities.each do |activity| %>
			
			<% if activity.trackable %>
		
			<div class="span10 well well-small">
			<p> 
				<img src=<%=activity.owner.avatar.small_thumb.url%>> 
				<%= link_to activity.owner.username, activity.owner if activity.owner %>
				<%= render_activity activity %>
				<p><%= format_date(activity.created_at) %></p>
			</p>
		</div>



			<% end %>

		<% end %>

		<% end %>



		<%= will_paginate @activities %>

