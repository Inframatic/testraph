<h1>Editing track</h1>

<%= simple_form_for(@track, html: { class: "form-horizontal"}) do |f| %>
  <%= f.error_notification %>

<%= f.input :title %>
<%= f.input :description %>

<div class="form-actions">
	<%= f.button :submit, class: "btn btn-primary" %>
</div>

  <% end %>

<%= link_to 'Show', @track %>

